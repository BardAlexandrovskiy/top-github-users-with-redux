  "private": true,
  "homepage": "http://BardAlexandrovskiy.github.io/top-github-users-width-redux",
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.0",
    "react-scripts": "3.1.1",
    "redux": "^4.0.4",
    "styled-components": "^4.3.2"
  },
  "scripts": {
