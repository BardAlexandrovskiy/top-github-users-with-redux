    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.1.0",
    "gh-pages": "^2.1.1",
    "prettier": "^1.18.2",
    "redux-logger": "^3.0.6"
